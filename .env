## all db passwords
# NOTE: when changing database passwords, one must also change the passwords in the init file (/volumes/postgres/init/dump.sql)
SERVICE_REGISTRY_DB_PASSWORD=registry

## Datasource urls
SERVICE_REGISTRY_DATASOURCE_URL=jdbc:postgresql://postgres:5432/service_registry

## Ports that will be exposed to the host (does not affect docker-network configurations)
PGADMIN_PORT=8091
SERVICE_REGISTRY_PORT=8085
POSTGRES_PORT=5432

## Versions
SERVICE_REGISTRY_VERSION=20231020
